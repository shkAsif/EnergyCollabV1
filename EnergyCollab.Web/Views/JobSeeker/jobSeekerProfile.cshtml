@using EnergyCollab.Web.Models
@model CandidateProfileDto

<div class="my-profile-container">
    <!-- <section id="section-home" class="page-section"> -->
    <div class="row">
        <div class="pull-left col-md-8">
            <h3>Update My Professional Profile</h3>
        </div>
        <div class="go-back-btn col-md-4">
            <button class="btn btn-success pull-right">
                <i class="fa fa-arrow-left"></i> Go Back
            </button>
        </div>
    </div>
    <hr>
    <div class="profile-info-contain">
        <div class="alert alert-info alert-sizing" data-bind="text: $root.loc.textCandidateProfileTip">
            Please take
            the time to fill in all mandatory fields, this process ensures our Partners can quickly match you to
            their Vacancies.
        </div>
        <div class="accordion" id="contac-info-accordian">
            <div class="item">
                <div class="accordion-item accordion-flush">
                    <h2 class="accordion-header">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
                                data-bs-target="#contact" aria-expanded="false" aria-controls="flush-collapseOne">
                            Contact Information
                        </button>
                    </h2>
                    <div id="contact" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                        <div class="accordion-body">
                            <form class="form form-admin">
                                <div class="form-group">
                                    <label for="inputCountry">Country:</label>
                                    <code>Required</code>
                                    <div class="form-group col-md-4 col-sm-12 col-xs-12">
                                        @Html.DropDownList("CountryCode", ViewData["Countries"] as List<SelectListItem>, "Select a country", new { @class = "form-control p-2"  , Name = "CountryCode"})
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="inputCityName">City Name:</label>
                                    <code>Required</code>
                                    <input type="text" class="form-control" id="inputCityName" required=""
                                           placeholder="City Name">
                                    <p class="small help-text validation-summary-errors"
                                       data-bind="validationMessage: cityName">
                                    </p>
                                </div>

                                <div class="form-group">
                                    <label for="inputAddressOne">
                                        Address
                                        One:
                                    </label>
                                    <code data-bind="text: $root.loc.stringRequired">Required</code>
                                    <textarea class="form-control" id="inputAddressOne" value=@Model.AddressOne
                                              placeholder="Address One">@Model.AddressOne</textarea>
                                    <p class="small help-text validation-summary-errors">
                                    </p>
                                </div>

                                <div class="form-group">
                                    <label for="inputAddressTwo"
                                           data-bind="text: $root.loc.stringAddressTwoColon">
                                        Address
                                        Two:
                                    </label>
                                    <textarea class="form-control" id="inputAddressTwo"
                                              data-bind="value: addressTwo, attr: { placeholder: $root.loc.placeholderAddressTwo }"
                                              placeholder="Address Two">@Model.AddressTwo</textarea>
                                </div>

                                <div class="form-group">
                                    <label for="inputPostCode"
                                           data-bind="text: $root.lostyc.stringPostCodeColon">
                                        Post
                                        Code:
                                    </label>
                                    <code data-bind="text: $root.loc.stringRequired">Required</code>
                                    <input type="text" class="form-control" id="inputPostCode"
                                           placeholder="Post Code" value="@Model.PostCode">

                                    <p class="small help-text validation-summary-errors"
                                       data-bind="validationMessage: postCode">
                                    </p>
                                </div>

                                <div class="form-group">
                                    <label for="inputPrimaryContactNumber"
                                           data-bind="text: $root.loc.stringPrimaryContactNumberColon">
                                        Primary
                                        Contact Number:
                                    </label>
                                    <code data-bind="text: $root.loc.stringRequired">Required</code>
                                    <input type="text" class="form-control" id="inputPrimaryContactNumber"
                                           placeholder="Primary Contact Number" value="@Model.PrimaryContactNumber">

                                    <p class="small help-text validation-summary-errors"
                                       data-bind="validationMessage: primaryContactNumber"></p>
                                </div>

                                <div class="form-group">
                                    <label for="inputSeconadaryContactNumber"
                                           data-bind="text: $root.loc.stringSecondaryContactNumberColon">
                                        Secondary
                                        Contact Number:
                                    </label>
                                    <input type="text" class="form-control" id="inputSeconadaryContactNumber"
                                           placeholder="Secondary Contact Number" value="@Model.SecondaryContactNumber">
                                </div>
                            </form>
                            <div class="mt-2 admin-button-panel text-right">
                                <button class="btn btn-success">Next</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
